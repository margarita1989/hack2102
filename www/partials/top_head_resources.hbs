<link rel="stylesheet" href="css/main.css"/>

<script>
    (function(global) {
        global.bookFace = {
            isLoggedIn: "{{ isLoggedIn }}",
            user: {
                name: "{{ user.google.name }}",
                books: [
                    {{# each user.books }}
                        {
                            "name": "{{ this.name }}",
                            "url": "{{ this.url }}"
                        },
                    {{/ each }}
                ]
            }
        };
    })(window);
</script>

<script src="lib/require.js" data-main="require-config"></script>